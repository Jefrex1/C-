#include <iostream>

int main() {
    int matrix[7][7];

    // Зчитуємо елементи матриці
    std::cout << "Введіть елементи матриці:\n";
    for (int i = 0; i < 7; i++) {
        for (int j = 0; j < 7; j++) {
            matrix[i][j] = rand();
        }
    }

    // Знайдемо добуток непарних елементів кожного стовпця
    for (int j = 0; j < 7; j++) {
        int product = 1;
        for (int i = 0; i < 7; i++) {
            if (matrix[i][j] % 2 != 0) {
                product *= matrix[i][j];
            }
        }
        std::cout << "Добуток непарних елементів у стовпці " << j + 1 << ": " << product << std::endl;
    }

    return 0;
}
